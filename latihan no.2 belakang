#include <iostream>

#include <math.h>

using namespace std;

struct titik {
	int x, y;
};

int main() {
	struct titik p1, p2;
	double jarak;
	cout << "titik A \nKoordinat x : ";
	cin >> p1.x;
	cout << "Koordinat y : ";
	cin >> p1.y;
	cout << endl;
	cout << "titik B \nKoordinat x : ";
	cin >> p2.x;
	cout << "Koordinat y : ";
	cin >> p2.y;
	cout << endl;
	jarak = sqrt(pow((p1.x - p2.x), 2) + pow((p1.y - p2.y), 2));
	cout << "Jarak titik A dan titik B = " << jarak;
  return 0;
}
