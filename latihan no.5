#include <iostream>
#include <math.h>

using namespace std;

void tanggal(int h, int b, int t) {
	int h2;
	h2 = h - 1;
	cout << "Kemarin : " << h2 << "-" << b << "-" << t << endl;
}

int main() {
	int h, b, t;
	cout << "Tanggal : ";
	cin >> h;
	cout << "Bulan : ";
	cin >> b;
	cout << "Tahun : ";
	cin >> t;
	
	if (h >= 32) {
		cout << endl;
		cout << "Tanggal tidak diketahui";
		return 0;
	}
	if (b >= 13) {
		cout << endl;
		cout << "bulan tidak diketahui";
		return 0;
	}

	cout << endl;
	cout << "Sekarang : " << h << "-" << b << "-" << t << endl;
	tanggal(h, b, t);
	
	return 0;
}
